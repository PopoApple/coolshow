<view class="page-body">
  <view class="page-body-wrapper">
    <map
      id="myMap"
      style="width: 100%; height: 500px;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      covers="{{covers}}"
      show-location
    ></map>
    <cover-image class="cover-image" bindtap="tap1" src="../../../../image/location.png" />
  </view>
</view>
